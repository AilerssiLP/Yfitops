<Window x:Class="Yfitops.MusicianWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Musician Panel" Height="400" Width="600"
        WindowStartupLocation="CenterScreen">

    <StackPanel Margin="20">

        <TextBlock Text="Musician Panel" FontSize="22" Margin="0 0 0 20"/>

        <Button Content="Create Album" Height="40" Margin="0 10" Click="CreateAlbum_Click"/>
        <Button Content="Add Song to Album" Height="40" Margin="0 10" Click="AddSong_Click"/>
        <Button Content="Logout" Height="40" Margin="0 10" Click="Logout_Click"/>

    </StackPanel>

</Window>
